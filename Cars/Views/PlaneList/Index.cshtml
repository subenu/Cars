@model IEnumerable<PlaneList>  

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Plane list</h2>
        </div>
        <div class="col-6 text-right">
            <a asp-controller="PlaneList" asp-action="Create" class="btn btn-primary">
                Create New Plane
            </a>
    </div>
</div>
    </div>
<br />

@if (Model.Count() > 0)
{
    <table class="table table-bordered table striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Name
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
            <tr>
                <td width=100%> @obj.Name</td>
            </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No Planes Found</p>
}
